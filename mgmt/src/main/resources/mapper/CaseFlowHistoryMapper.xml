<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.managementsystem.mapper.CaseFlowHistoryMapper">

    <select id="selectByCaseId" resultType="com.example.managementsystem.entity.CaseFlowHistory">
        SELECT * FROM case_flow_history
        WHERE case_id = #{caseId}
        ORDER BY create_time DESC
    </select>

</mapper>